<%- include("include/head.ejs")  %>

<body>
    <h2>This page is belongs to <%= data.name%>
    </h2>
    <button>Follow</button>
    <button>Messages</button>

    <p>Follower : <%= data.followers%> &nbsp; &nbsp; &nbsp; Following : <%= data.following%>
    </p>

    <hr>

    <% for( let post of data.posts ) { %>
    <img src=" <%= post.image %>" alt="some image" width="150px" height="180px" />
    <p>Likes : <%= post.likes%> &nbsp; &nbsp; &nbsp; Comments : <%= post.comments %>
    </p>
    <% } %>
            
</body>
<%- include("include/footer.ejs")  %>
</html>